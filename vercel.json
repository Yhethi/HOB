{
  "version": 2,
  "builds": [
    { "src": "api/price.py", "use": "@vercel/python" },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/price.py" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}
